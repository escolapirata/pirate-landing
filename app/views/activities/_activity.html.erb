<div class="vertical-timeline-block">
    <div class="vertical-timeline-icon navy-bg">
      <i class="fa fa-plus"></i>
    </div>

    <div class="vertical-timeline-content ibox">
        <div class="ibox-header">
            <%= image_tag activity.image.url(:cover), class:"img-responsive" %>
        </div>
        <h2>
            <%= link_to activity.name, activity_path(activity)%></br>
            <small>26 ABR 18:30 - Laboratório Aberto do ITA - CCM</small>
        </h2>
          <ul class="tag-list" style="padding: 0">
                <li><a href="">Inovação</a></li>
                <li><a href="">Empreendedorismo</a></li>
                <li><a href="">Workshop</a></li>
          </ul>
        <div class="hr-line-dashed"></div>
        <p><%= activity.description %></p>
    </div>
</div>
